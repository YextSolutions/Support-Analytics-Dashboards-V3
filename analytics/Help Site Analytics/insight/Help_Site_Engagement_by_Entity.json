{
  "$id": "supportAnalytics_Help_Site_Engagement_by_Entity",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Engagement by Entity",
  "description": "Track Search KPIs by Entity to determine entities that are most commonly returned and most commonly engaged with.",
  "query": {
    "metrics": [
      "yext/ANSWERS_SEARCHES",
      "yext/ANSWERS_CLICKS",
      "yext/ANSWERS_CLICK_THROUGH_RATE"
    ],
    "fields": [
      "entityId",
      "name",
      "address",
      "yext/derivedField-postalCode",
      "yext/derivedField-city"
    ],
    "dimensions": [
      "yext/ENTITIES"
    ],
    "filters": [
      {
        "filter": "yext/ANSWERS_CONFIGURATION_VERSION_LABEL",
        "in": [
          "PRODUCTION"
        ]
      },
      {
        "filter": "yext/ANSWERS_QUERY_SOURCE",
        "in": [
          "HELP_SITE",
          "SUPPORT_SEARCH"
        ]
      },
      {
        "filter": "yext/IS_HUMAN",
        "in": [
          "true"
        ]
      }
    ]
  },
  "display": {
    "table": {
      "sort": {
        "column": {
          "metric": "yext/ANSWERS_CLICK_THROUGH_RATE"
        },
        "descending": true
      }
    }
  }
}