{
  "$id": "supportAnalytics_Searches_by_Search_Term_Word_Count_Table",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Searches, Knowledge Graph Result Rate, and Search Click Through Rate by Search T",
  "description": "Searches, Knowledge Graph Result Rate, and Search Click Through Rate by Search Term Word Count, currently includes autocomplete and user-typed searches",
  "query": {
    "metrics": [
      "yext/ANSWERS_SEARCHES",
      "yext/ANSWERS_KG_RESULT_RATE",
      "yext/ANSWERS_CLICK_THROUGH_RATE"
    ],
    "dimensions": [
      "yext/ANSWERS_SEARCH_TERM_WORD_COUNT"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "DAYS_14"
      },
      {
        "filter": "yext/ANSWERS_QUERY_SOURCE",
        "in": [
          "AGENT_DESKTOP"
        ]
      },
      {
        "filter": "yext/ANSWERS_CONFIGURATION_VERSION_LABEL",
        "in": [
          "PRODUCTION"
        ]
      }
    ]
  },
  "display": {
    "table": {
      "sort": {
        "column": {
          "metric": "yext/ANSWERS_SEARCHES"
        },
        "descending": true
      }
    }
  }
}