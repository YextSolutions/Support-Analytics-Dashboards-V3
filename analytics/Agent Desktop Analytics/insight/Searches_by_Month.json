{
  "$id": "supportAnalytics_Agent_Desktop_Searches_by_Month",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Searches by Month",
  "description": "Shows the overall searches on Agent Desktop Search over the past 12 months. Currently includes both subject line autocompletion and agent typed searches.",
  "query": {
    "metrics": [
      "yext/ANSWERS_SEARCHES"
    ],
    "dimensions": [
      "yext/MONTHS"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "MONTHS_12"
      },
      {
        "filter": "yext/ANSWERS_QUERY_SOURCE",
        "in": [
          "AGENT_DESKTOP"
        ]
      },
      {
        "filter": "yext/ANSWERS_CONFIGURATION_VERSION_LABEL",
        "in": [
          "PRODUCTION"
        ]
      }
    ]
  },
  "display": {
    "line": {}
  }
}