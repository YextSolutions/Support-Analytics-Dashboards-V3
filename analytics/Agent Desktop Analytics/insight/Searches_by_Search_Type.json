{
    "$id": "supportAnalytics_Searches_by_Search_Type",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Searches by Search Type (Placeholder)",
    "description": "Differentiates Searches by Search Type Auto Searches vs. Agent-initiated Searches.",
    "query": {
      "metrics": [
        "yext/ANSWERS_SEARCHES"
      ],
      "dimensions": [
        "yext/ANSWERS_FILTER_SOURCE"
      ],
      "filters": [
        {
          "filter": "yext/date",
          "in": "DAYS_14"
        },
        {
          "filter": "yext/ANSWERS_QUERY_SOURCE",
          "in": [
            "AGENT_DESKTOP"
          ]
        },
        {
          "filter": "yext/ANSWERS_CONFIGURATION_VERSION_LABEL",
          "in": [
            "PRODUCTION"
          ]
        }
      ]
    },
    "display": {
      "table": {
        "sort": {
          "column": {
            "metric": "yext/ANSWERS_SEARCHES"
          }
        }
      }
    }
  }