{
  "$id": "supportAnalytics_Agent_Desktop_Searches_by_Search_Term_Word_Count_Graph",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Searches by Search Term Word Count",
  "description": "Searches by Search Term Word Count, currently includes autocompletion and agent typed searches",
  "query": {
    "metrics": [
      "yext/ANSWERS_SEARCHES"
    ],
    "dimensions": [
      "yext/ANSWERS_SEARCH_TERM_WORD_COUNT"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "DAYS_14"
      },
      {
        "filter": "yext/ANSWERS_QUERY_SOURCE",
        "in": [
          "AGENT_DESKTOP"
        ]
      },
      {
        "filter": "yext/ANSWERS_CONFIGURATION_VERSION_LABEL",
        "in": [
          "PRODUCTION"
        ]
      }
    ]
  },
  "display": {
    "line": {}
  }
}