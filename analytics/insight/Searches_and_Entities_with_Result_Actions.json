{
    "$id": "supportAnalytics_Searches_and_Entities_with_Result_Actions",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Searches and Entities with Result Actions (Placeholder)",
    "description": "Searches and Search Clicks by Search Term, Entity, and Result Action",
    "query": {
      "metrics": [
        "yext/ANSWERS_SEARCHES",
        "yext/ANSWERS_CLICKS"
      ],
      "fields": [
        "entityId",
        "name",
        "address",
        "yext/derivedField-postalCode",
        "yext/derivedField-city"
      ],
      "dimensions": [
        "yext/ANSWERS_SEARCH_TERM",
        "yext/ENTITIES"
      ],
      "filters": [
        {
          "filter": "yext/date",
          "in": "DAYS_14"
        },
        {
          "filter": "yext/ANSWERS_QUERY_SOURCE",
          "in": [
            "AGENT_DESKTOP"
          ]
        },
        {
          "filter": "yext/ANSWERS_CONFIGURATION_VERSION_LABEL",
          "in": [
            "PRODUCTION"
          ]
        }
      ]
    },
    "display": {
      "table": {
        "sort": {
          "column": {
            "metric": "yext/ANSWERS_SEARCHES"
          },
          "descending": true
        }
      }
    }
  }